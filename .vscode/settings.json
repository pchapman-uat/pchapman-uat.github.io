{
    "cSpell.words": [
        "Susic"
    ],
    "cSpell.ignorePaths": [
        "package-lock.json",
        "node_modules",
        "vscode-extension",
        ".git/{info,lfs,logs,refs,objects}/**",
        ".git/{index,*refs,*HEAD}",
        ".vscode",
        ".vscode-insiders",
        ".github",
        ".gitignore"
    ],
    "editor.codeActionsOnSave": {
        "source.organizeImports": "always"
    },
}